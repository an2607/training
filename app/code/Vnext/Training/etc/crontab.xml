<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="defaultEmail">
        <job instance="Vnext\Training\Cron\SendMail" method="execute" name="mageplaza_helloworld_cron">
            <schedule>* * * * *</schedule>
        </job>
    </group>
    <group id="testCron">
        <job instance="Vnext\Training\Cron\Test" method="execute" name="emailTest">
            <schedule>* * * * *</schedule>
        </job>
    </group>
</config>
